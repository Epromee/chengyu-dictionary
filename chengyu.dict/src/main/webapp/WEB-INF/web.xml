<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>dictionaryServlet</servlet-name>
    <servlet-class>com.colewe.ws1819.server.DictionaryServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>dictionaryServlet</servlet-name>
    <url-pattern>/semesterproject/dictionary</url-pattern>
  </servlet-mapping>
  
  

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>semesterproject.html</welcome-file>
  </welcome-file-list>

</web-app>
